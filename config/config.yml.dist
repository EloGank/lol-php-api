config:
    cache:
      path:           cache

    log:
      path:           logs/api.log
      verbosity:      debug
      max_file:       5

    client:
      async:
        enabled:      false
        startPort:    5000
        redis:
          host:       127.0.0.1
          port:       6379
          key:        elogank.api
      request:
        timeout:      5
        overload:
          available:  0.03
          wait:       4
      response:
        expire:       60
      authentication:
        busy:
          wait:       30
      version:        4.6.x
      locale:         en_US
      accounts:
        - region:     ~
          username:   ~
          password:   ~
          async:
            port:     ~

    region:
      class:          EloGank\Api\Model\Region\Region
      servers:
        NA:
          name:       North America
          server:     prod.na1.lol.riotgames.com
          loginQueue: lq.na1.lol.riotgames.com
        EUW:
          name:       Europe West
          server:     prod.eu.lol.riotgames.com
          loginQueue: lq.eu.lol.riotgames.com

    server:
        port:         8080
        bind:         127.0.0.1
        format:       json